type PurchaseOrder {
	id: ID!,
	productType: String,
	quantity: Int,
	imageUrl: String,
	customerID: Int
}

type Query {
	purchaseOrders(count : Int):[PurchaseOrder]
	purchaseOrder(id:ID):PurchaseOrder
}

type Mutation {
	createPurchaseOrder(productType: String!, quantity: Int!, imageUrl: String!,  customerID: Int!):PurchaseOrder
}